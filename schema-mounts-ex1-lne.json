"ietf-yang-library:yang-library": {
  ...
  "module-set": [
    {
      "name": "lne-modules",
       ...
    },
    {
      "name": "ni-modules",
      "module": [
        {
          "name": "ietf-ipv4-unicast-routing",
          "revision": "2016-11-04",
          "namespace":
            "urn:ietf:params:xml:ns:yang:ietf-ipv4-unicast-routing"
        },
        {
          "name": "ietf-ipv6-unicast-routing",
          "revision": "2016-11-04",
          "namespace":
           "urn:ietf:params:xml:ns:yang:ietf-ipv6-unicast-routing"
        },
        {
          "name": "ietf-routing",
          "revision": "2016-11-04",
          "feature": [
            {
              "name": "multiple-ribs"
            },
            {
              "name": "router-id"
            }
          ],
          "namespace": "urn:ietf:params:xml:ns:yang:ietf-routing"
        }
      ]
    }
  ],
  "schema": [
    {
      "name": "lne-schema",
      "module-set": [ "lne-modules" ],
      "ietf-yang-schema-mount:schema-mounts": {
        "namespace": [
          {
            "prefix": "if",
            "uri": "urn:ietf:params:xml:ns:yang:ietf-interfaces"
          },
          {
            "prefix": "ni",
            "uri":
              "urn:ietf:params:xml:ns:yang:ietf-network-instance"
          }
        ],
        "mount-point": [
          {
            "module": "ietf-network-instance",
            "label": "root",
            "use-schema": {
                "name": "ni-schema",
                "parent-reference": [
                  "/if:interfaces/if:interface[
                      ni:bind-network-instance-name =
                      current()/../ni:name]"
                ]
            }
          }
        ]
      }
    },
    {
      "name": "ni-schema",
      "module-set": [ "ni-modules" ]
    }
  ],
  ...
}
